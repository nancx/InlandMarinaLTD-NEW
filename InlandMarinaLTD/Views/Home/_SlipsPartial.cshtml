@model List<Slip>

@if (Model.Count > 0)
{
    <table class="table">
        <thead>
            <tr>
                <th>Slip ID</th>
                <th>Width</th>
                <th>Length</th>
                <th>Dock</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var slip in Model)
            {
                <tr>
                    <td>@slip.ID</td>
                    <td>@slip.Width</td>
                    <td>@slip.Length</td>
                    <td>@slip.Dock.Name</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No available slips found.</p>
}